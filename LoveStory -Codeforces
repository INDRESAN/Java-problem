import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int input=sc.nextInt();
		sc.nextLine();
		for(int i=0;i<input;i++) {
			String s=sc.next();
			int n=s.length();
			String s1="codeforces";
			int c=0;
			for(int j=0;j<n;j++) {
				if(s.charAt(j)!=s1.charAt(j)) {
					c++;
				}
			}
			System.out.println(c);
		}
	}
}
